                    Return_Value
                [/PARAMLITERAL]
                [PARAMLITERAL:title]
                    Event Name
                [/PARAMLITERAL]
				[PARAMLITERAL:value]
                    Item.Equipped
                [/PARAMLITERAL]
            [/OBJECT]
			[OBJECT:BlueprintPin]
                [PARAMLITERAL:type]
                    string
                [/PARAMLITERAL]
                [PARAMLITERAL:id]
